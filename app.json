{
  "expo": {
    "name": "Find my Friends",
    "slug": "findmyfriends",
    "owner": "ai-camp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.aicamp.findmyfriends",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.aicamp.findmyfriends",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#8A8AF4"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "Find your Friends!",
    "githubUrl": "https://github.com/organization-x/find-your-friends"
  }
}
