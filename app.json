        "googleSignIn" : {
          "reservedClientId" : "com.googleusercontent.apps.523384189636-b5lmlo27s13fhd6o1qa02kujbrm25ubd"
        }
      }
    },
    "android": {
      "package": "com.aicamp.findmyfriends",
      "versionCode": 2,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/ARGaiaIconAndroid.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "Find your Friends in AR!",
    "githubUrl": "https://github.com/organization-x/find-your-friends"
